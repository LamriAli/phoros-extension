{
    "name": "Phoros-extension",
    "description": "destined to start the social media extraction process",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/icons/phoros-white-bg.png",
            "32": "/icons/phoros-white-bg.png",
            "48": "/icons/phoros-white-bg.png",
            "128": "/icons/phoros-white-bg.png"
        }
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "/icons/phoros-white-bg.png",
        "32": "/icons/phoros-white-bg.png",
        "48": "/icons/phoros-white-bg.png",
        "128": "/icons/phoros-white-bg.png"
    }
}